{
    "name": "Open Deezer in desktop app",
    "description": "Get redirected to the Deezer desktop app when you open a Deezer link in the browser.",
    "version": "1.2.0",
    "manifest_version": 2,
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "content_scripts": [
      {
        "matches": [
          "https://www.deezer.com/*"
        ],
        "all_frames": true,
        "js": ["content-script.js"]
      }
    ]
  }
